{
  "name": "nodejs",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "dev": "nodemon ./src/server.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "start": "node dist/src/server.js",
    "build": "tsc --project ./",
    "migration:run": "npx typeorm-ts-node-commonjs migration:run -d src/data-source.ts",
    "migration:revert": "npx typeorm-ts-node-commonjs migration:revert -d src/data-source.ts",
    "migration:show": "yarn run typeorm migration:show -d src/data-source.ts",
    "migration:generate": "yarn run typeorm migration:generate -d src/data-source.ts",
    "migration:create": "yarn run typeorm migration:create",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.1",
    "@types/express": "^4.17.18",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/node": "^20.8.7",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
